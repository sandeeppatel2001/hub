# Session Manager Configuration

# Redis connection
REDIS_URL=redis://localhost:6379

# Docker image for browser containers
BROWSER_IMAGE=browser-interceptor:latest

# Port ranges for dynamic allocation
BASE_NOVNC_PORT=8100
BASE_CDP_PORT=9300

# Session timeout in seconds (default: 2 hours)
SESSION_TIMEOUT=7200

# Base URL for generating session links
BASE_URL=http://localhost

# S3/MinIO configuration (optional)
S3_BUCKET=browser-logs
S3_ENDPOINT=http://localhost:9000
AWS_ACCESS_KEY_ID=admin
AWS_SECRET_ACCESS_KEY=your-secure-password

# Server port
PORT=3000
